terraform {
  backend "gcs" {
    bucket  = "${state_file_bucket_name}"
    prefix  = "${state_file_bucket_prefix}"
  }

  required_version = ">= ${tf_version}"

  required_providers {
%{ for provider in providers ~}
    ${lower(provider.name)} = {
      source  = "${provider.provider_source}"
      version = ">= ${provider.provider_version}"
    }
%{ endfor ~}
  }
}
